@model IEnumerable<MVCPractice.Models.userDetailViewModel>

@{
    ViewBag.Title = "Index";


}


@using (Html.BeginForm())
{
    <h2>Customers Data</h2>
    <div class="jumbotron d-inline-block" style=" margin-top: 100px">
        <div class="table-responsive">

            <div>

                <table border="1" style="font-family:Arial" class="table table-hover table-striped">
                    <tr>
                   
                        <th>Customer Id</th>
                        <th>Customer Name</th>
                        <th>Phone Number </th>
                        <th>Branch Id</th>
                        <th>Manager Id</th>
                    </tr>

                    @foreach (MVCPractice.Models.userDetailViewModel customer in @Model)
                    {
                        <tr>
                            
                            <td>
                                @customer.userId
                            </td>
                            <td>
                                @customer.userName
                            </td>
                            <td>
                                @customer.phoneNumber
                            </td>
                            <td>
                                @customer.branchId
                            </td>
                            <td>
                                @customer.managerId
                            </td>
                            
                        </tr>

                    }

                </table>
                <br />

            </div>

        </div>

    </div>
}


