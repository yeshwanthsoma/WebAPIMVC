@model BusinessLayer.Manager

@{
    ViewBag.Title = "AddManager";
}

@using (Html.BeginForm())
{
    <div class="jumbotron bg-light text-dark">
        <div>
            <h1><label id="Label1">Add Manager</label></h1>
        </div>
        <div class="form-group">
            <label id="Label2" class="col-form-label">Manager Name</label>
            <input type="text" class="form-control" name="name" id="name" />
            @*@Html.Editor("name")*@
        </div>

        <div class="form-group">
            <label id="Label4" class="col-form-label">Branch Id</label>
            <input type="text" class="form-control" name="branch" id="branch" />
            @*@Html.Editor("branch")*@
        </div>

        <div class="form-group">
            <label id="Label4" class="col-form-label">Address</label>
            <input type="text" class="form-control" name="address" id="address" />
            @*@Html.Editor("address")*@
        </div>

        <div class="form-group">
            <label id="Label4" class="col-form-label">Phone Number</label>
            <input type="text" class="form-control" name="phno" id="phno" />
            @*@Html.Editor("phno")*@
        </div>

        <div class="form-group">
            <label id="Label4" class="col-form-label">Email Id</label>
            <input type="text" class="form-control" name="mail" id="mail" />
            @*@Html.Editor("mail")*@
        </div>
        <div>
            <input type="submit" class="btn btn-outline-success" value="Add Manager" />
            <button id="back" onclick="location.href='@Url.Action("ManageManager","SuperManager")'" class="btn btn-outline-success">Back</button>
        </div>
        <label id="Label6"></label>
        <div>
            @ViewBag.result
        </div>

    </div>
}



